<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ProductService WSDL service -->

  <service name="Product" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://DefaultNamespace"/>
      <parameter name="wsdlServiceElement" value="ProductService"/>
      <parameter name="schemaQualified" value="http://DefaultNamespace"/>
      <parameter name="wsdlServicePort" value="Product"/>
      <parameter name="className" value="DefaultNamespace.ProductSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Product"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="addProduct" qname="operNS:addProduct" xmlns:operNS="http://DefaultNamespace" returnQName="retNS:addProductReturn" xmlns:retNS="http://DefaultNamespace" returnType="rtns:ArrayOf_xsd_anyType" xmlns:rtns="http://DefaultNamespace" returnItemQName="tns:item" xmlns:tns="http://DefaultNamespace" soapAction="" >
        <parameter qname="pns:name" xmlns:pns="http://DefaultNamespace" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:id" xmlns:pns="http://DefaultNamespace" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:price" xmlns:pns="http://DefaultNamespace" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="addProduct"/>

      <typeMapping
        xmlns:ns="http://DefaultNamespace"
        qname="ns:>addProductResponse"
        type="java:DefaultNamespace.AddProductResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <typeMapping
        xmlns:ns="http://DefaultNamespace"
        qname="ns:>addProduct"
        type="java:DefaultNamespace.AddProduct"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
      <arrayMapping
        xmlns:ns="http://DefaultNamespace"
        qname="ns:ArrayOf_xsd_anyType"
        type="java:java.lang.Object[]"
        innerType="cmp-ns:anyType" xmlns:cmp-ns="http://www.w3.org/2001/XMLSchema"
        encodingStyle=""
      />
  </service>
</deployment>
